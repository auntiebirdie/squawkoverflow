  <meta property="og:site_name" content="SQUAWKoverflow">
  <meta name="theme-color" content="#DDF600">

 <% if (locals.page && page == 'aviary') { %>

  <meta property="og:title" content="<%= member.username %>'s Aviary">
  <meta property="og:description" content="There are over <%= (member.totals.aviary * 1).toLocaleString("en-US") %> birds in <%= member.username %>'s aviary!">
  <meta property="og:image" content="<%= member.avatar ? member.avatar : "/img/avatar-placeholder.jpeg" %>">

 <% } else if (locals.page && page == 'flock') { %>

  <meta property="og:title" content="<%= flock.name %>">
  <meta property="og:description" content="<%= flock.description ? flock.description : `${member.username}'s flock contains ${flock.totals.birdypets} birds.` %>">
  <meta property="og:image" content="<%= member.avatar ? member.avatar : "/img/avatar-placeholder.jpeg" %>">

 <% } else if (locals.page && page == 'member') { %>

  <meta property="og:title" content="<%= member.username %>">
  <meta property="og:description" content="<%= member.username %> is a <%= member.rank %> <%= member.title %> with over <%= (member.totals.aviary * 1).toLocaleString("en-US") %> birds in <%= member.preferredPronoun.cases.determiner %> aviary! %>">
  <meta property="og:image" content="<%= member.avatar ? member.avatar : '/img/avatar-placeholder.jpeg' %>">

 <% } else if (locals.birdypet && locals.member) { %>

  <meta property="og:title" content="<%= birdypet.nickname ? birdypet.nickname : `${member.username}'s ${birdypet.bird.commonName}` %>">
  <meta property="og:description" content="<% if (birdypet.description) { %><%= birdypet.description %> <% } else { %><%= birdypet.bird.scientificName %><% } %>">
  <meta property="og:image" content="<%= birdypet.variant.image %>">
  <meta property="twitter:card" content="summary_large_image">
  <meta property="twitter:image" content="<%= birdypet.variant.image %>">

 <% } else if (locals.page && page == 'birdypedia/bird') { %>

  <meta property="og:title" content="<%= bird.commonName %>">
  <meta property="og:image" content="<%= bird.variants[0].image %>">
  <meta property="twitter:card" content="summary_large_image">
  <meta property="twitter:image" content="<%= bird.variants[0].image %>">

 <% } else { %>

  <meta property="og:title" content="SQUAWKoverflow - the ultimate bird collecting game">
  <meta property="og:description" content="Blue eggs, green eggs, tiny eggs, massive eggsâ€”we have them all! With over 23,000 birds you can hatch, every egg is an opportunity to discover a new species.">
  <meta property="og:image" content="/img/SQUAWK.png">

 <% } %>

