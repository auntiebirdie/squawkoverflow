        <% if (locals.page && page == 'aviary') { %>
            <meta property="og:title" content="<%= member.username %>'s Aviary">
            <meta property="og:image" content="<%= member.avatar ? member.avatar : "/img/avatar-placeholder.jpeg" %>">
        <% } else if (locals.page && page == 'flock') { %>
            <meta property="og:title" content="<%= flock.name %>">
            <meta property="og:description" content="<%= member.username %>'s Flock">
            <meta property="og:image" content="<% member.avatar %>">
        <% } else if (locals.page && page == 'member') { %>
            <meta property="og:title" content="<%= member.username %>">
            <meta property="og:image" content="<%= member.avatar ? member.avatar : "/img/avatar-placeholder.jpeg" %>">
            <meta property="og:type" content="profile">
            <meta property="profile:username" content="<%= member.username %>">
            <meta property="profile:first_name" content="<%= member.username %>">
            <meta property="profile:last_name" content="">
        <% } else if (locals.memberpet && locals.member) { %>
            <meta property="og:title" content="<%= memberpet.nickname ? memberpet.nickname : memberpet.species.commonName %>">
            <meta property="og:description" content="<% if (memberpet.description) { %><%= memberpet.description %> <% } else { %><%= member.username %>'s <%= memberpet.species.commonName %><% } %>">
            <meta property="og:image" content="<%= memberpet.image %>">
            <meta property="og:type" content="article">
            <meta property="article:author" content="https://squawkoverflow.com/members/<%= member._id %>">
            <meta property="twitter:card" content="summary_large_image">
            <meta property="twitter:image" content="<%= memberpet.image %>">
        <% } else if (locals.page && page == 'birdypedia/bird') { %>
            <meta property="og:title" content="<%= bird.commonName %>">
            <meta property="og:image" content="<%= birdypets[0].image %>">
            <meta property="twitter:card" content="summary_large_image">
            <meta property="twitter:image" content="<%= birdypets[0].image %>">
        <% } else { %>
