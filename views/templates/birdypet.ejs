<div class="col-6 col-md-4">
  <div class="pb-4">
    <div class="card <% if (locals.loggedInUser && locals.page && page == 'flock' && loggedInUser.id == member._id) { %>${bird.flocks.includes('<%= flock._id %>') ? 'border-chartreuse' : 'border-secondary border-dashed'}<% } %>">
      <% if (locals.page && page == 'gift') { %>
        <button class="wishlist-heart btn btn-light btn-sm" role="button">
          ${bird.wishlisted ? '❤️' : '🤍'}
        </button>
      <% } else { %>
        <a class="wishlist-heart">
          ${bird.wishlisted ? '❤️' : ''}
        </a>
      <% } %>
      <div class="lazy birdypet checkmark${bird.checkmark}"
           data-id="${bird._id}"
           data-bg="${bird.image}"
      ></div>
      <h3 class="text-center"><a href="/birdypet/${bird._id}">${bird.nickname ? bird.nickname : bird.species.commonName}</a></h3> 
      <h6 class="text-center">${bird.nickname ? bird.species.commonName : (`${bird.version}&nbsp;${bird.label}`)}</h6>
    </div>
  </div>
</div>
