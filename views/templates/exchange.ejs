<div class="col-6 col-md-4 d-flex align-items-stretch">
  <div class="w-100 pb-3 px-2">
    <div class="h-100 card p-1 ${model.exchangeData.length > 0 ? (model.exchangeData.find((data) => data.id == '<%= exchange.id %>') ? 'border-primary' : 'disabled') : 'border-secondary border-dashed border-2'}">
      <div class="birdypet-actions">
        <a class="wishlist-heart">
          ${model.bird.wishlisted ? (model.bird.wishlisted == 2 ? 'ğŸŒŸ' : 'â¤ï¸') : ''}
        </a>
      </div>
      <div>
        <div class="bg-white lazy birdypet checkmark${model.bird.owned > 2 ? 2 : model.bird.owned}"
             data-id="${model.id}"
             data-bg="${model.variant.image}"
        ></div>
      </div>
      <div class="card-body align-top">
        <h3 class="text-center">${model.bird.commonName}</h3> 
        <h5 class="text-center">${model.nickname ? model.nickname : '&nbsp;'}</h5>
        <p class="text-center">${model.exchangeData.length == 0 || model.exchangeData.find((data) => data.id == '<%= exchange.id %>') ? '' : (model.member == '<%= loggedInUser.id %>' ? `<a href="/exchange/${model.exchangeData[0].id}">in another exchange</a>` : 'in another exchange')}</p>
      </div>
    </div>
  </div>
</div>
