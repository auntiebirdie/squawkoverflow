<div class="col-md-3 d-flex align-items-stretch">
  <div class="w-100 pb-4">
    <div class="h-100 card" style="cursor: pointer">
      <a class="wishlist-heart">
        ${bird.wishlisted ? '❤️' : '' }
      </a>
      <div>
        <div class="bg-white lazy birdypet checkmark${bird.owned > 2 ? 2 : bird.owned}"
             data-id="${bird.freebirdId}"
             data-birdypet="${bird.id}"
             data-bg="${bird.image}"
        ></div>
      </div>
      <div class="card-body">
        <h3 class="mt-3 text-center">${bird.species.commonName}</h3>
        <h6 class="mb-3 text-center">${bird.label}</h6>
      </div>
    </div>
  </div>
</div>
