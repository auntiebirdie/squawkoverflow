<div class="col-6 col-md-4 d-flex align-items-stretch">
  <div class="w-100 pb-3 px-2">
    <div class="h-100 card" style="cursor: pointer">
      <div class="birdypet-actions">
        <a class="wishlist-heart">
          ${model.bird.wishlisted ? (model.bird.wishlisted == 2 ? 'üåü' : '‚ù§Ô∏è') : ''}
        </a>
      </div>
      <div>
        <div class="bg-white lazy birdypet checkmark${model.bird.owned > 2 ? 2 : model.bird.owned}"
             data-freebird="${model.freebird}"
             data-variant="${model.id}"
             data-bg="${model.image}"
        ></div>
      </div>
      <div class="card-body">
        <h3 class="text-center">${model.nickname ? model.nickname : model.bird.commonName}</h3>
        <h6 class="text-small text-center">${model.nickname ? model.bird.commonName : model.bird.scientificName}</h6>
      </div>
    </div>
  </div>
</div>
