<div class="col-6 col-md-4 d-flex align-items-stretch">
  <div class="w-100 pb-3 px-2">
    <div class="h-100 card">
      <a class="wishlist-heart">
        ${model.bird.wishlisted ? (model.bird.wishlisted == 2 ? 'üåü' : '‚ù§Ô∏è') : ''}
      </a>
      <div>
        <a href="/birdypet/${model.id}/gift?member=<%= member.id %>">
          <div class="bg-white lazy birdypet checkmark${model.bird.owned > 2 ? 2 : model.bird.owned}"
               data-id="${model.id}"
               data-bg="${model.variant.image}"
          ></div>
        </a>
      </div>
      <div class="card-body align-top">
        <h3 class="text-center"><a href="/birdypet/${model.id}/gift?member=<%= member.id %>">${model.nickname ? model.nickname : model.bird.commonName}</a></h3> 
        <h6 class="text-center">${model.nickname ? model.bird.commonName : (model.variant.label ? model.variant.label : '&nbsp;')}</h6>
      </div>
    </div>
  </div>
</div>
