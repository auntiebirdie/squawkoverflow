  <div class="row mx-3 mb-4 p-3 rounded bg-light">
    <div class="col-2 g-0 text-center">
      <img class="rounded" src="${model.avatar}" onerror="missingMemberAvatar(this)" width="64">
    </div>
    <div class="col-8">
      <h4 class="text-break"><a href="/members/${model.id}">${model.username}</a></h4>
      <h6>${model.rank} ${model.title}</h6>
      <p class="small mb-0"><em>Last Active: ${model.lastActive}</em></p>
    </div>
    <div class="col-2 d-flex">
      <span class="small text-center align-self-center">üê¶<br>${Intl.NumberFormat('en', { notation : 'compact' }).format(model.aviary * 1)}</span>
    </div>
  </div>
</div>
