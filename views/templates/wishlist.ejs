<div class="col-6 col-md-4 d-flex align-items-stretch">
  <div class="w-100 pb-3 px-2">
    <div class="h-100 card">      
      <div class="birdypet-actions">
       <% if (locals.loggedInUser && loggedInUser.id == member.id) { %>
        <button class="wishlist-heart btn btn-sm" data-id="${model.id}" role="button">
          ${model.wishlisted ? (model.wishlisted == 2 ? '🌟' : '❤️') : '🤍'}
        </button>
       <% } %>
      </div>
      <div>
        <div class="bg-white lazy birdypet ${model.owned ? "hatched" : "unhatched"} checkmark${model.owned == 2 ? 2 : model.owned}"
             data-bg="${model.variants[0].image}"
        ></div>
      </div>
      <div class="card-body">
        <h3 class="text-center">
          <a
            <% if (locals.loggedInUser) { %>
            <% if (loggedInUser.id == member.id) { %>href="${model.owned ? `/aviary/<%= loggedInUser.id %>?search=%22${model.commonName}%22` : `/birdypedia/bird/${model.id_slug}`}"<% } %>
            <% if (loggedInUser.id != member.id) { %>href="${model.ownedd ? `/members/<%= member.id %>/gift?search=%22${model.commonName}%22` : `/birdypedia/bird/${model.id_slug}`}"<% } %>
            <% } else { %>
             href="/birdypedia/bird/${model.id_slug}"
            <% } %>
          >
            ${model.commonName}
          </a>
        </h3> 
      </div>
    </div>
  </div>
</div>
